document.getElementById('profilecontainer').innerHTML="<% @profiles.each do |profile| %><div class='col-md-6 col-sm-6 col-xs-12' align='middle'><div class='card mb-10' style='width: 95%;'> <% if profile.picture!=''%><div class='card-img-top' style='background-image: url(<%= profile.picture %>);'></div><% else %><div class='card-img-top' style='background-image: url(/default.jpg);'></div><% end %><div class='card-body text-left'><ul class='list-group card-ul'><li class='name list-group-item'> <%= profile.fname %></li><li class='type list-group-item'><i class='fas fa-home'></i> <%= profile.room_type %></li><li class='address list-group-item'><i class='fas fa-map-marker'></i> <%= profile.address %></li><li class='price pt-2 list-group-item'><i class='fas fa-rupee-sign'></i> <%= profile.price %></li><li class='phone list-group-item'><i class='fas fa-phone'></i> <%= profile.phone %></li><% user =  User.find(profile.user_id) %><li class='email list-group-item'><i class='fas fa-envelope'></i> <%= user.email %></li></ul></div></div></div><% end %>"